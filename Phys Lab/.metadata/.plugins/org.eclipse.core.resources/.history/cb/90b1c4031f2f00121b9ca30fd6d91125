package utils.graphing;

public class Point 
{
	private int x;
	private int y;
	
	public Point()
	{
		
	}
	
	public Point setX(int x)
	{
		this.x = x;
		return this;
	}
	
	public Point setY(int y)
	{
		this.y = y;
		return this;
	}
	
	public int getX()
	{
		return x;
	}
	
	public int getY()
	{
		return y;
	}
}
